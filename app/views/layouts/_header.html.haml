= image_tag ("CClogo.png", :class=>"logo")
#nav
	%ul
		-if signed_in?
			%li= link_to 'Upload', upload_path
			%li= link_to 'Sign Out', signout_path, :method => :delete
			-unless current_user.notifications.empty?
				%li= link_to current_user.notifications.count, '/notifications'
		-else
			%li= link_to 'Sign In', signin_path
			%li= link_to 'Home', root_path