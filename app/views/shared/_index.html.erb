<% if signed_in?%>
		

		
<%= paginate @photos %>
		
  
	<% unless @photos.nil?%>
		<% @photos.each do |photo| %>
		
    		<div class="listedpic">
		    <center><%= link_to image_tag (photo.image_url(:collage).to_s), photo, :class=> "zoom" %></center>
				</div>
	
		<% end %>
		

		
		<%else%>
		<h1>There are no photos yet!</h1>
		<%end%>
<%else%>
<%= render 'shared/signin'%>
<%end%>


